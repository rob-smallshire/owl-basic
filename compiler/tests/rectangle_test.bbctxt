REM Test of RECTANGLE statement

REM Draw a rectangle
RECTANGLE xpos, ypos, width, height
RECTANGLE 100, 200, 300, 400

REM Draw a filled rectangle
RECTANGLE FILL xpos, ypos, width, height
RECTANGLE FILL 640, 512, 256, 128

REM Copy a rectangle
RECTANGLE xpos, ypos, width, height TO destx, desty
RECTANGLE 100, 100, 50, 50, TO 200, 200

REM Move a rectangle
RECTANGLE FILL xpos, xpos, width, height TO destx, desty
RECTANGLE FILL 123, 456, 321, 654 TO 23, 47

REM Swap two rectangles
RECTANGLE SWAP x1, y1, dx, dy TO x2, y2
RECTANGLE SWAP 10, 20, 5, 7, 678, 232


